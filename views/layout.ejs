<!doctype html>
<html>
<head>
<title>LOVE MAP</title>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css"> 
<link rel="stylesheet" href="/stylesheets/style.css">
</head> 
  
<body>
  
	<nav class="navbar navbar-default navbar-fixed-top">
	<div class="container-fluid">
		<div class="navbar-header">
		  <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
			<span class="sr-only">Toggle navigation</span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
		  </button>
		  <a class="navbar-brand" href="">LOVE MAP</a>
		</div>

		<div class="collapse navbar-collapse" id="navbar-collapse-1">
		  <ul class="nav navbar-nav">	  
			<li><a href="#">Options</a></li> 
			<li class="dropdown">
			  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Links <span class="caret"></span></a>
			  <ul class="dropdown-menu">
				<li><a href="http://www.edubrooks.com/" target="_blank">My homepage</a></li>
				<li role="separator" class="divider"></li>
				<li><a href="https://github.com/edujbrooks/love-map" target="_blank">Github project</a></li>
			  </ul>
			</li>
		  </ul>
		</div><!-- /.navbar-collapse -->
	</div><!-- /.container-fluid -->
	</nav>
  
  
    <div id="map-canvas"></div>
	<div id="tweetFrame" class="tweetFrame front"></div>
	
	<!-- JS -->
	<script src="https://code.jquery.com/jquery-1.11.1.js"></script>
	<script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
	<script src="http://google-maps-utility-library-v3.googlecode.com/svn/trunk/infobox/src/infobox.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
	<script src="/socket.io/socket.io.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/mustache.js/0.7.2/mustache.min.js"></script>
	<script src="/javascripts/js.js"></script>

	<!-- moustache templates -->
	<script id="tweet-template" type="text/html">
		<div class="tweetDeck">
			<div class="tweetHeader">
				<img class="tweetAvatar" src="{{avatar}}" alt="avatar">
				<span class="tweetName">{{name}}</span><br/>
				<span class="tweetNickname"><a href="http://www.twitter.com/{{screenName}}" target="_blank">@{{screenName}}</a></span>
			</div>
			<div class="tweetBody">
				{{{body}}}
			</div>
			{{#picUrl}}
				<a href="{{picLink}}" target="_blank"><img src="{{picUrl}}" alt=""></a>
			{{/picUrl}}
		</div>
	</script>
	<script id="infowindow-template" type="text/html">
		<div class="infobox-wrapper">
			<div id="infobox">
				<div class="tweetHeader">
					<img class="tweetAvatar" src="{{avatar}}" alt="avatar">
					<span class="tweetName">{{name}}</span><br/>
					<span class="tweetNickname"><a href="http://www.twitter.com/{{screenName}}" target="_blank">@{{screenName}}</a></span>
				</div>
				<div class="tweetBody">
					{{{body}}}
				</div>
				{{#picUrl}}
					<a href="{{picLink}}" target="_blank"><img class="infowindowImg" src="{{picUrl}}" alt=""></a>
				{{/picUrl}}
			</div>
		</div>
	</script>
	<!-- end moustache templates -->
  </body>
</html>